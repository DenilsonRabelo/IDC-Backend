generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["teste"]
}

// Usuário administrativo
    model UsuarioTeste {
    id String @id @default(uuid())

    @@map("usuario")
    @@schema("teste")
    }

// Assistido 
    model AssistidoTeste {
    id String @id @default(uuid())

    @@map("assistido")
    @@schema("teste")
    }

// Relacionados com assistido
    // Endereço
    model EnderecoTeste{
    id String @id @default(uuid())

    @@map("endereco")
    @@schema("teste")
    }
    // Responsavel
    model ResponsavelTeste{
    id String @id @default(uuid())

    @@map("responsavel")
    @@schema("teste")
    }

// Familia
    model FamiliaTeste{
    id String @id @default(uuid())

    @@map("familia")
    @@schema("teste")
    }
// Relacionados Com Familia
    model FamiliarTeste{
    id String @id @default(uuid())

    @@map("familiar")
    @@schema("teste")
    }

    model DespesasTeste{
    id String @id @default(uuid())

    @@map("despesas")
    @@schema("teste")
    }

    model VeiculoTeste{
    id String @id @default(uuid())

    @@map("veiculo")
    @@schema("teste")
    }

    model HabitacaoTeste{
    id String @id @default(uuid())

    @@map("habitacao")
    @@schema("teste")
    }

